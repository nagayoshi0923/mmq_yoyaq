    "node": "20.x"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "build:fast": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 100",
    "verify": "npm run typecheck && npm run lint && npm run build:fast",
    "db:check": "node scripts/check_schema.js",
    "db:reset": "npx supabase db reset --linked",
    "db:diff": "npx supabase db diff --schema public",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
